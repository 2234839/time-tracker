{
  "applicationId": "dev.time.tracker",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "resources/dist/",
  "url2": "/index.html",
  "url": "http://localhost:5173",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "storage.*"
  ],
  "modes": {
    "window": {
      "title": "工时记录",
      "width": 900,
      "height": 700,
      "minWidth": 600,
      "minHeight": 500,
      "fullScreen": false,
      "alwaysOnTop": false,
      "enableInspector": true,
      "openInspectorOnStartup": true,
      "injectGlobals": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "time-tracker",
    "resourcesPath": "resources/",
    "extensionsPath": "extensions/",
    "clientLibrary": "resources/js/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "3.17.0",
    "frontendLibrary": {
      "patchFile": "/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/",
      "initCommand": "pnpm install",
      "devCommand": "pnpm dev",
      "buildCommand": "pnpm build"
    }
  }
}